    "rxjs": "^6.1.0",
    "style-loader": "^0.21.0",
    "todomvc-app-css": "^2.1.2",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "zone.js": "^0.8.26"
