{
  "name": "telekinetic",
  "version": "0.11.2",
  "description": "Transparent reactive programming for React.",
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./lib/ && tsc",
    "test": "jest",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephenbunch/telekinetic.git"
  },
  "author": "Stephen Bunch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stephenbunch/telekinetic/issues"
  },
  "homepage": "https://github.com/stephenbunch/telekinetic#readme",
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^22.2.2",
    "@types/react": "^16.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.8.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js",
      "tsx"
    ],
    "testRegex": "\\.spec\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "peerDependencies": {
    "react": "^16.x"
  }
}
